{
  "name": "codeup/encoding",
  "type": "library",
  "description": "collection of encoding schemes in order to encode and decode based on different strategies",
  "homepage": "https://github.com/codeup-berlin/encoding",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Codeup\\Encoding\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codeup\\Encoding\\": "tests"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-bcmath": "*",
    "ext-sodium": "*",
    "spomky-labs/php-aes-gcm": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.5"
  },
  "scripts": {
    "test": "phpunit -c tests/phpunit.xml"
  }
}
